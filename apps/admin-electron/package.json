{
  "name": "qieos-admin-electron",
  "version": "1.0.0",
  "description": "QiEOS Admin Cockpit - Desktop application for administrators",
  "main": "dist/main.js",
  "scripts": {
    "start": "npm run build && electron dist/main.js",
    "dev": "concurrently \"npm run dev:main\" \"npm run dev:renderer\"",
    "dev:main": "tsc -p tsconfig.main.json --watch",
    "dev:renderer": "vite",
    "build": "npm run build:main && npm run build:renderer",
    "build:main": "tsc -p tsconfig.main.json",
    "build:renderer": "vite build",
    "dist": "npm run build && electron-builder",
    "pack": "npm run build && electron-builder --dir"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-router-dom": "^6.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^4.0.0",
    "concurrently": "^8.0.0",
    "electron": "^38.1.0",
    "electron-builder": "^24.0.0",
    "typescript": "^5.0.0",
    "vite": "^4.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/qiallyme/qieos-admin-electron.git"
  },
  "keywords": [
    "admin"
  ],
  "author": "QiAlly",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/qiallyme/qieos-admin-electron/issues"
  },
  "homepage": "https://github.com/qiallyme/qieos-admin-electron#readme"
}
